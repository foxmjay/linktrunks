{
 "manifest_version": 2,
 "name": "LinkTrunks",
 "description": "Fastest and easiest way to manage your bookmarks and links",
 "version": "1",
 "author": "LinkTrunks",

"browser_action": {
   "default_icon": "tab-icon.png",
   "default_title": "LinkTrunks",
    "default_popup": "linktrunks_popup.html"

 },



"content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["content.js"]
   }
 ],

 "permissions": ["<all_urls>","activeTab","tabs","storage","notifications"]
}
